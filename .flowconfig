[ignore]
<PROJECT_ROOT>/coverage/.*
<PROJECT_ROOT>/packages/.*/build/.*
[lints]
all=warn
untyped-import=off
sketchy-null-bool=error
[options]
emoji=true
include_warnings=true
experimental.const_params=true
module.ignore_non_literal_requires=true
module.use_strict=true
[libs]
node_modules/@flows/makethen/src
node_modules/@flows/globby/src
node_modules/@flows/execa/src
node_modules/@flows/babel/src
node_modules/@flows/prettier-eslint/src